<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

 

    <!-- bean definitions here -->

 <bean id="address1" class="org.question1.Address">
      <property name="street" value="4, Shivajinagar, "></property> 
      <property name="city" value="Pune, "></property> 
      <property name="state" value="MH, "></property> 
      <property name="country" value="India-"></property> 
      <property name="zip" value="411005."></property> 
 </bean>
 
 <bean id="customer1" class="org.question1.Customer">
      <property name="customerID" value="10"></property> 
      <property name="customerName" value="Aditi, "></property> 
      <property name="customerContact" value="8600423421, "></property> 
      <property name="customerAddress" ref="address1"></property> 
 </bean>

</beans>

